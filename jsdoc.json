{
	"plugins": [],
	"recurseDepth": 10,
	"source": {
        "include": ["./index.js"],
		"exclude": ["node_modules/"],
		"includePattern": ".+\\.js(doc|x)?$",
		"excludePattern": "(^|\\/|\\\\)(__|_).*"
	},
	"sourceType": "module",
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc", "closure"]
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false
	}
}
